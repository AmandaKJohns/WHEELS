<h1>My Trip</h1>
<h3>Origin: <%=@trip.origin.address%></h3>
<h3>Destination: <%=@trip.destination.address %></h3>
<h3>Cost Estimate: <%= @cost %></h3>
<h3>Time Estimate: <%= @time %></h3>


<h3>Uber Rides:</h3>
  <ul> 
    <% @uber_rides.each do |ride| %>
    <li>
    <p>Level: <%= ride["localized_display_name"] %></p>
    <p>Duration:  <%= ride["duration"] %></p>
    <p>Estimated Cost: <%= ride["estimate"] %></p>
    </li>
    <% end %>
  </ul>


<h3>Lyft Cost Estimate: <%= @lyft_cost %></h3>
<h3>Lyft Time Estimate: <%= @lyft_time %></h3>




<%= link_to 'New Search', root_path %>
